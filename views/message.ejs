<html lang="en">

<head>
	<title>About</title>
    <% include ./partials/head %>
</head>

<body class="landing-page">
    <!-- Navbar -->
    <% include ./partials/nav-connected-orange %>
    <!-- End Navbar -->
    <div class="wrapper">
        <div class="page-header page-header-small">
            <div class="page-header-image" data-parallax="true" style="background-image: url('/img/bg-12.jpg');">
            </div>
            <div class="container">
                <div class="content-center">
                    <h1 class="title">Private Messages</h1>
                </div>
            </div>
        </div>
        <div class="section">
            <div class="container">
                <div class="row">
					<div class="card">
                                <ul class="nav nav-tabs justify-content-center" role="tablist">
                                    <li class="nav-item">
                                        <a class="nav-link active" data-toggle="tab" href="#messages" role="tab">
                                            <i class="fa fa-envelope-o"></i> Messages
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link" data-toggle="tab" href="#friends-message" role="tab">
                                            <i class="fa fa-users"></i> Friends
                                        </a>
                                    </li>
                                </ul>
                                <div class="card-body">
                                    <!-- Tab panes -->
                                    <div class="tab-content text-center">
                                        <div class="tab-pane active" id="messages" role="tabpanel">
											<table style="width:100%">
											  <tr>
											    <th>Firstname</th>
											    <th>Lastname</th>
											    <th>Age</th>
											  </tr>
											  <tr>
											    <td>Jill</td>
											    <td>Smith</td>
											    <td>50</td>
											  </tr>
											  <tr>
											    <td>Eve</td>
											    <td>Jackson</td>
											    <td>94</td>
											  </tr>
											</table>
                                        </div>
                                        <div class="tab-pane" id="friends-message" role="tabpanel">
                                            <% include ./partials/friends-partial %>
                                        </div>
                                    </div>
                                </div>
                            </div>
                </div>
            </div>
        </div>
    </div>
</body>
<% include ./partials/footer %>

</html>
